#############################################################################################
# Message Templates for send_curl function
#
# Todo:
# qr-code
# exe in zip
# exe in iso
# makro in zip
# exe in iso
# activex in office
# oleoject in office
# vbscript, jsscript, python, perl, java ..
# eml nested attachments
# pyzor, razor hit
# Phishing
# office97 with makro
#############################################################################################
compose_message() {
case "$switch" in
#############################################################################################
# Eicar Virus Test
virus)
cat > "$mail" <<EOF
X-SPAM: NO
Subject: eicar AV Test
To: <"$each">
From: 'Evil Sender' <evil@evil-domain.com>
Message-Id: <$(date "+%m%d%H%M%Y")-114054033-4711@$helo>
Date: $(date -R)
MIME-Version: 1.0
Content-Type: multipart/mixed;
 boundary="------------11C9D15A08B345C2A6100327"
Content-Language: de-DE

This is a multi-part message in MIME format.
--------------11C9D15A08B345C2A6100327
Content-Type: text/plain; charset=utf-8
Content-Transfer-Encoding: 7bit


--------------11C9D15A08B345C2A6100327
Content-Type: application/octet-stream; x-mac-type="54455854"; x-mac-creator="522A6368";
 name="eicar.com"
Content-Transfer-Encoding: base64
Content-Disposition: attachment;
 filename="eicar.com"

WDVPIVAlQEFQWzRcUFpYNTQoUF4pN0NDKTd9JEVJQ0FSLVNUQU5EQVJELUFOVElWSVJVUy1U
RVNULUZJTEUhJEgrSCo=
--------------11C9D15A08B345C2A6100327--
EOF
;;
#############################################################################################
# GTUBE Spamtest
spam)
cat > "$mail" <<EOF  
X-SPAM: NO
Subject: spam message with gtube
To: <"$each">
From: 'Evil Sender' <evil@evil-domain.com>
Message-Id: <$(date "+%m%d%H%M%Y")-114054033-4711@$helo>
Date: $(date -R)
MIME-Version: 1.0
Content-Type: text/plain; charset=utf-8; format=flowed
Content-Transfer-Encoding: 8bit
Content-Language: en-US

Hallo "$each"
XJS*C4JDBQADN1.NSBN3*2IDNEN*GTUBE-STANDARD-ANTI-UBE-TEST-EMAIL*C.34X
EOF
;;
#############################################################################################
# executable attachment
exe)
cat > "$mail" <<EOF  
X-SPAM: NO
Subject: exe attachment
To: <"$each">
From: 'Evil Sender' <evil@evil-domain.com>
Message-Id: <$(date "+%m%d%H%M%Y")-114054033-4711@$helo>
Date: $(date -R)
MIME-Version: 1.0
Content-Type: multipart/mixed;
 boundary="------------F0724A537730ECB93BC9CEB4"
Content-Language: de-DE

This is a multi-part message in MIME format.
--------------F0724A537730ECB93BC9CEB4
Content-Type: text/html; charset=utf-8
Content-Transfer-Encoding: 7bit

<html>
  <head>

    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
  </head>
  <body>
    <p><br>
    </p>
  </body>
</html>

--------------F0724A537730ECB93BC9CEB4
Content-Type: application/zip; x-mac-type="0"; x-mac-creator="0";
 name="attachment.zip"
Content-Transfer-Encoding: base64
Content-Disposition: attachment;
 filename="attachment.zip"

UEsDBBQAAAAIAJxYq1AQcc19UwAAAFcAAAAJABwAc2NyaXB0LnNoVVQJAAM3FbleOBW5XnV4
CwABBPUBAAAEFAAAAFNW1E/KzNNPSizO4FJWcCwtycgvslIILU9VCM7PzU0tAgoWgxkOeakl
yfl5ukBcXJpTkpmXrpecn8uVmpyRr6AUnFyUWVCikJtaXJyYnqrExQUAUEsBAh4DFAAAAAgA
nFirUBBxzX1TAAAAVwAAAAkAGAAAAAAAAQAAAO2BAAAAAHNjcmlwdC5zaFVUBQADNxW5XnV4
CwABBPUBAAAEFAAAAFBLBQYAAAAAAQABAE8AAACWAAAAAAA=
--------------F0724A537730ECB93BC9CEB4
Content-Type: application/x-sh; x-mac-type="0"; x-mac-creator="0";
 name="script.sh"
Content-Transfer-Encoding: 7bit
Content-Disposition: attachment;
 filename="script.sh"

#!/bin/bash
echo "Script message"

--------------F0724A537730ECB93BC9CEB4--
EOF
;;
#############################################################################################
# html Mail with tracker
html)
cat > "$mail" <<EOF  
X-SPAM: NO
Subject: html message
To: <"$each">
From: 'Evil Sender' <evil@evil-domain.com>
Message-Id: <$(date "+%m%d%H%M%Y")-114054033-4711@$helo>
Date: $(date -R)
MIME-Version: 1.0

Hallo "$each"
EOF
;;
# office macro attachment
#############################################################################################
macro)
cat > "$mail" <<EOF  
Subject: office macro attachment
To: <"$each">
From: 'Evil Sender' <evil@evil-domain.com>
Message-Id: <$(date "+%m%d%H%M%Y")-114054033-4711@$helo>
Date: $(date -R)
MIME-Version: 1.0
Content-Type: multipart/mixed;
 boundary="------------D59C128D75773CB14097244B"
Content-Language: de-DE

This is a multi-part message in MIME format.
--------------D59C128D75773CB14097244B
Content-Type: text/plain; charset=utf-8
Content-Transfer-Encoding: 7bit


Testmessage from evil sender

--------------D59C128D75773CB14097244B
Content-Type: application/vnd.oasis.opendocument.text; x-mac-type="0"; x-mac-creator="0";
 name="file_with_macro.odt"
Content-Transfer-Encoding: base64
