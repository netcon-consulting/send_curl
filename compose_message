# detect IP or hostname and send spoofmessage
*.*.*)
cat > "$mail" <<EOF  
X-SPAM: NO
Subject: Gruesse aus Berlin
To: <"$each">
From: 'Angela Merkel' <bundeskanzler@bund.de>
Reply-to: <evil@evil-domain.com>
Message-Id: <$(date "+%m%d%H%M%Y")-114054033-4711@$helo>
Date: $(date -R)
MIME-Version: 1.0
Content-Type: text/plain; charset=utf-8; format=flowed
Content-Transfer-Encoding: 8bit
Content-Language: en-US

Hallo "$each"

Liebe Gruesse aus Berlin
Deine Angela
EOF
;;
#############################################################################################
# Bulk Sender
[1-9]|[1-9][0-9])
cat > "$mail" <<EOF  
X-SPAM: NO
Subject: Massmailing Message Number: $COUNTER of $switch
To: <"$each">
From: 'Evil Sender' <evil@evil-domain.com>
Message-Id: <$(date "+%m%d%H%M%Y")-0815@$helo>
Date: $(date -R)
MIME-Version: 1.0
Content-Type: text/plain; charset=utf-8; format=flowed
Content-Transfer-Encoding: 8bit
Content-Language: en-US

Hallo "$each"
Message Number $COUNTER of $switch
EOF
;;
#############################################################################################
